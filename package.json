{
  "name": "vue-elt",
  "version": "1.4.11",
  "description": "Vue Single File Components Library",
  "keywords": [
    "javascript",
    "vue",
    "component",
    "single-file-component",
    "library"
  ],
  "license": "Apache-2.0 License",
  "main": "dist/vue-elt.ssr.js",
  "browser": "dist/vue-elt.esm.js",
  "module": "dist/vue-elt.esm.js",
  "unpkg": "dist/vue-elt.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "repository": "github:philippebeck/vue-elt",
  "homepage": "https://github.com/philippebeck/vue-elt",
  "bugs": "https://github.com/philippebeck/vue-elt/issues",
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "test": "jest --env=jsdom"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "testEnvironmentOptions": {
      "customExportConditions": ["node", "node-addons"]
  },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/@vue/vue3-jest"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@tinymce/tinymce-vue": "^5.0.0",
    "animadio": "^6.2.5",
    "servidio": "^3.0.0",
    "vue-recaptcha": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/test-utils": "^2.3.2",
    "@vue/vue3-jest": "^29.2.4",
    "babel-jest": "^29.5.0",
    "cross-env": "^7.0.3",
    "grunt": "^1.3.0",
    "grunt-contrib-concat": "^2.1.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jsdom": "^21.1.1",
    "minimist": "^1.2.5",
    "postcss": "^8.2.10",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^3.3.8"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "engines": {
    "node": ">=12"
  },
  "author": {
    "name": "Philippe Beck",
    "email": "philippe@philippebeck.net",
    "url": "https://philippebeck.net"
  }
}
